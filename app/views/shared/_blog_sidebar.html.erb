<div class="col-sm-3 offset-sm-1 blog-sidebar">
  <div class="sidebar-module sidebar-module-inset">
    <h3>Categories</h3>
    <% @side_bar_categories.each do |category| %>
      <p class='category-list'><%= link_to category.title, category_show_path(category) %></p>
    <% end %>
    <ol class="list-unstyled social-links">
      <li><a href="https://github.com/cpalka87"><%= fa_icon "github" %></a></li>
      <li><a href="https://www.instagram.com/palkaphoto/"><%= fa_icon "instagram" %></a></li>
    </ol>
  </div>
</div>
